<!DOCTYPE html/>
<html>
<head>
	<title><%= title %></title>
	<link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
	<h1>Users</h1>
	<p>User List</p>
	<div id="users">
		<% users.forEach(function(user) { %>
		<div class="users">
			<h2><%=user.fullName()%></h2>
		</div>
		<% }) %>
	</div>
	<select id="user">
		<% users.forEach(function(user) { %>
		<option value = "<%=user.fullName()%>"><%=user.fullName()%></option>
		<% }) %>

	</select>
</body>
</html>

